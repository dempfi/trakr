@require 'constants'
global-reset()

::-webkit-scrollbar
  display none
  width 0

html
  box-sizing border-box
  font-size 13px

body
  font-family 'Helvetica Neue'
  color OXFORD
  overflow hidden

input
  background transparent
  border none

*
*:before
*:after
  box-sizing inherit

a
  text-decoration none
  color inherit
